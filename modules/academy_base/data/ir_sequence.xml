<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data noupdate="0">

        <record id="ir_sequence_academy_action_enrolment" model="ir.sequence">
            <field name="name">Academy action enrolment sequence</field>
            <field name="active" eval="True" />
            <field name="code">academy.action.enrolment.sequence</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="number_next" eval="1" />
            <field name="number_increment" eval="1" />
            <field name="padding" eval="4" />
            <field name="suffix"></field>
            <field name="company_id" eval="False" />
        </record>

        <record id="ir_sequence_academy_lesson" model="ir.sequence">
            <field name="name">Academy lesson sequence</field>
            <field name="active" eval="True" />
            <field name="code">academy.lesson.sequence</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="number_next" eval="1" />
            <field name="number_increment" eval="1" />
            <field name="padding" eval="4" />
            <field name="suffix"></field>
            <field name="company_id" eval="False" />
        </record>

    </data>
</openerp>
